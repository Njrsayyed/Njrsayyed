/*
10.A find the fibonacci series for given number
*/
// fibonacci series  - 0 1 1 2 3 5 8 13 .....
#include <stdio.h>
int main() 
{
    int n;
    printf("enter the number ");
    scanf("%d",&n);
    int  a=0;
    int  b=1;
    int  c=0;
    for ( int i=1 ; i <=n; i++)
    { 
        printf("%d", a );
        c=a+b;
         a=b;
         b=c;
    }
}
