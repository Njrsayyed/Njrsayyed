 // funnction the name with no return type and  argument
#include <stdio.h>

int add() {
    int a,b;
    int sum;
    printf("enter the value of a ");
    scanf ("%d",&a);
    printf("enter the value of b ");
    scanf("%d",&b);
    sum = a+b;
    return sum;
}
    void main() {
    int sum;
    sum=add();
    printf("%d",sum);
}

output
enter the value of a 25
enter the value of b 25
50
